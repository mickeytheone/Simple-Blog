<%- include("partials/header"); -%> 

<h1>Compose</h1>
<form class="" action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle" placeholder="New Title" autocomplete="off">
    </div>
    <div class="form-group">
        <label>Content</label>
        <textarea class="form-control" name="postBody" rows=5  placeholder="New Content"></textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer"); -%>
